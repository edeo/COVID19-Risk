'''' insert data function)

# To Create table in Postgres
CREATE TABLE NY_US_COUNTIES
(
  date date,
  county character varying(50),
  state character varying(50),
  dob date,
  fips number(10),
  cases number(15),
  deaths number(15),
  confirmed_cases number(15),
  confirmed_deaths number(15),
  probable_cases number(15),
  probable_deaths number(15),
  cases number(15) character varying(255)
)

# To import this CSV file into the persons table, you use COPY statement as follows:

COPY persons(first_name,last_name,dob,email) 
FROM 'C:\tmp\persons.csv' DELIMITER ',' CSV HEADER;